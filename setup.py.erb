#!/usr/bin/env python
# encoding: UTF-8

from distutils.core import setup

setup(
    name = '<%= name %>',
    version = '<%= version %>',
    author = '<%= author %>',
    author_email = '<%= email %>',
    packages = ['<%= package %>'],
    url = 'http://pypi.python.org/pypi/<%= name %>/',
    license = '<%= license %>',
    description = '<%= description %>',
    long_description=open('README.rst').read(),
)

